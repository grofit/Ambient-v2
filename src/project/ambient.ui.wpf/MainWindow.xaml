<Window x:Class="ambient.ui.wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:players="clr-namespace:ambient.ui.wpf.players"
        Title="MainWindow" Height="350" Width="560">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="skins/default/DefaultSkin.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <StackPanel Name="MainContainer" Style="{DynamicResource MainContainerStyle}">
        <WrapPanel>
            <players:AmbientPlayer></players:AmbientPlayer>
            <players:AmbientPlayer></players:AmbientPlayer>
            <players:AmbientPlayer></players:AmbientPlayer>
            <players:AmbientPlayer></players:AmbientPlayer>
        </WrapPanel>
    </StackPanel>
</Window>
